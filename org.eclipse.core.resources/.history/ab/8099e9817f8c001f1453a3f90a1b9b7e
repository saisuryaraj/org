package utilities;
public class ConfigEncryptor {
    public static void main(String[] args) {
        try {
            String plainTextPassword = "yourPlainPassword"; // Replace with your actual password
            String encryptedPassword = EncryptionUtil.encrypt(plainTextPassword);
            System.out.println("Encrypted Password: " + encryptedPassword);
            // You can now manually add this encrypted password to the config.properties file
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
